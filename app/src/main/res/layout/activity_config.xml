<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="com.nhquang.parking3.ConfigAct">

    <CheckBox
        android:id="@+id/Config_chk_isipss"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="IS IPSS" />

    <EditText
        android:id="@+id/Config_edt_baseapiurl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="api url"/>
    <CheckBox
        android:id="@+id/Config_chk_isregistercard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="IS REGISTER CARD" />
    <Button
        android:id="@+id/Config_btn_save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SAVE"/>
    <EditText
        android:id="@+id/Config_edt_access_token"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="access token"/>
    <EditText
        android:id="@+id/Config_edt_token_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="token_type"/>
</LinearLayout>
